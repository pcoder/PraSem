<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE rdf:RDF [
    <!ENTITY component 'http://www.wouterbeek.com/prasem/component.owl#'>
    <!ENTITY expression 'http://www.wouterbeek.com/prasem/expression.owl#'>
    <!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
    <!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
    <!ENTITY xsd 'http://www.w3.org/2001/XMLSchema#'>
]>

<rdf:RDF
    xmlns:component="&component;"
    xmlns:expression="&expression;"
    xmlns:rdf="&rdf;"
    xmlns:rdfs="&rdfs;"
    xmlns:xsd="&xsd;"
>
<rdf:Description rdf:about="&component;derivative_transition">
  <rdfs:subClassOf rdf:resource="&component;state_transition"/>
  <rdfs:label xml:lang="en">derivative transition</rdfs:label>
  <component:can_instantiate rdf:datatype="&xsd;boolean">fail</component:can_instantiate>
</rdf:Description>

</rdf:RDF>
