<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE rdf:RDF [
    <!ENTITY expression 'http://www.wouterbeek.com/prasem/expression.owl#'>
    <!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
    <!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
    <!ENTITY xsd 'http://www.w3.org/2001/XMLSchema#'>
]>

<rdf:RDF
    xmlns:expression="&expression;"
    xmlns:rdf="&rdf;"
    xmlns:rdfs="&rdfs;"
    xmlns:xsd="&xsd;"
>
<rdf:Description rdf:about="&expression;derivative_equal_to">
  <rdfs:subClassOf rdf:resource="&expression;derivative_inequality"/>
  <rdfs:subClassOf rdf:resource="&expression;equal_to"/>
  <rdfs:label xml:lang="en">'s derivative is equal to</rdfs:label>
  <expression:has_conflicting_expression rdf:resource="&expression;derivative_greater_than"/>
  <expression:has_conflicting_expression rdf:resource="&expression;derivative_smaller_than"/>
  <expression:has_abbreviation xml:lang="en">d=</expression:has_abbreviation>
</rdf:Description>

</rdf:RDF>
